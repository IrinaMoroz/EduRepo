<UserControl x:Class="HelloWorldFr.InstaUserControlDemo.UserControls.PostLikedByUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             d:Background="Wheat"
             xmlns:local="clr-namespace:HelloWorldFr.InstaUserControlDemo.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" Margin="3,0,0,0">
        <TextBlock>Liked by</TextBlock>
        <TextBlock x:Name="CloseFried1"  Text="user 1," FontWeight="Bold" Margin="3,0,0,0" ></TextBlock>
        <TextBlock x:Name="CloseFried2" Text="user 2" FontWeight="Bold"  Margin="3,0,3,0" ></TextBlock>
        <TextBlock Text="and" Margin="3,0,3,0"></TextBlock>
        <TextBlock x:Name="OtherLikeCount" Text="45" Margin="3,0,3,0"></TextBlock>
        <TextBlock Text="other"></TextBlock>
    </StackPanel>
</UserControl>
